{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "parser": "babel-eslint",
  "rules": {
    "no-debugger": "off",
    "no-console": "off",
    "react/prop-types": "off",
    "no-unused-vars": "warn"
  },
  "ignorePatterns": [
    "dist"
  ],
  "globals": {
    "console": true,
    "window": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
