{
  "name": "taverne",
  "version": "1.13.0",
  "description": "Elementary Flux implementation for your state management in React",
  "repository": "https://github.com/uralys/taverne",
  "license": "MIT",
  "keywords": [
    "flux",
    "react",
    "state",
    "react-hooks"
  ],
  "author": "@chrisdugne",
  "main": "cjs/taverne.js",
  "module": "esm/taverne.js",
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf ./dist ./dist.package",
    "build": "node scripts/build.js",
    "build:package": "sh scripts/build-package.sh",
    "lint": "eslint src",
    "test": "ava",
    "prerelease": "npm run build && npm run build:package",
    "release": "npm publish ./dist.package"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0"
  },
  "dependencies": {
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-react": "^7.12.13",
    "@babel/register": "^7.13.8",
    "ava": "^3.15.0",
    "deep-equal": "^2.0.5",
    "immer": "^8.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "esbuild": "^0.8.51",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5"
  }
}
